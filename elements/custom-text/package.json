{
  "name": "@amyscript/custom-text",
  "wcfactory": {
    "className": "CustomText",
    "customElementClass": "LitElement",
    "elementName": "custom-text",
    "generator-wcfactory-version": "0.6.3",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/custom-text.css",
      "scss": "src/custom-text.scss",
      "html": "src/custom-text.html",
      "js": "src/custom-text.js",
      "properties": "src/custom-text-properties.json",
      "hax": "src/custom-text-hax.json"
    }
  },
  "version": "8.0.0",
  "description": "Text content for info card",
  "repository": {
    "type": "git",
    "url": "https://github.com/AmyScript/weather-app-components.git"
  },
  "main": "custom-text.js",
  "module": "custom-text.es6.js",
  "umd": "custom-text.umd.js",
  "amd": "custom-text.amd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@amyscript/custom-text/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "amyscript"
  },
  "license": "MIT",
  "dependencies": {
    "lit-element": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "0.0.35",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.0.2",
    "@webcomponents/webcomponentsjs": "2.2.1",
    "concurrently": "4.1.0",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.4",
    "lodash": "4.17.11",
    "polymer-build": "3.1.1",
    "polymer-cli": "1.9.1",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.1"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
