{
  "name": "@amyscript/weather-app",
  "version": "0.0.0",
  "description": "The weather app factory",
  "wcfactory": {
    "askSASS": true,
    "askProps": true,
    "askHAX": true,
    "monorepo": "wcf-factory",
    "orgNpm": "@cosmicdreams",
    "orgGit": "cosmicdreams",
    "gitRepo": "git@github.com:cosmicdreams/wcf-factory.git",
    "sass": {
      "name": "rh-sass",
      "pkg": "@rhelements/rh-sass",
      "path": "rh-sass/rh-sass"
    }
  },
  "author": {
    "name": "cosmicdreams",
    "url": ""
  },
  "license": "Apache-2.0",
  "repository": "git@github.com:cosmicdreams/wcf-factory.git",
  "scripts": {
    "start": "yarn run storybook",
    "lerna": "lerna",
    "init": "lerna init",
    "postinstall": "lerna link",
    "test": "yarn run build && wct --configFile wct.conf.json test/index.html",
    "build": "lerna run build",
    "publish": "echo 'NOTICE: You must run `lerna publish` manually'",
    "test-suite-inject": "node scripts/test-suite-inject.js",
    "storybook": "start-storybook -c .storybook -p 9229 -s ./elements",
    "build-storybook": "build-storybook -c .storybook -o ../../storybooks/wcf-factory",
    "rebuild-wcfcache": "./node_modules/.bin/gulp",
    "new": "yarn run rebuild-wcfcache && wcf element && yarn run test-suite-inject && yarn install --link-duplicates"
  },
  "devDependencies": {
    "@amyscript/dsg-card": "^4.0.0",
    "@amyscript/dsg-heading": "^4.0.0",
    "@amyscript/dsg-image": "^4.0.0",
    "@amyscript/dsg-text": "^4.0.0",
    "@babel/core": "7.1.5",
    "@babel/preset-env": "7.1.5",
    "@emotion/styled-base": "^0.10.6",
    "@storybook/addon-a11y": "4.0.4",
    "@storybook/addon-actions": "4.0.4",
    "@storybook/addon-knobs": "4.0.4",
    "@storybook/addon-notes": "4.0.4",
    "@storybook/addon-options": "4.0.4",
    "@storybook/addon-storysource": "4.0.4",
    "@storybook/addon-viewport": "4.0.4",
    "@storybook/addons": "4.0.4",
    "@storybook/cli": "4.0.4",
    "@storybook/polymer": "4.0.4",
    "@webcomponents/webcomponentsjs": "2.1.3",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-custom-element-classes": "0.1.0",
    "babel-plugin-transform-es2015-modules-umd": "6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2017": "^6.24.1",
    "browser-sync": "2.26.3",
    "chalk": "2.4.1",
    "css-scss": "0.1.1",
    "decomment": "0.9.2",
    "del": "3.0.0",
    "dialog-polyfill": "0.4.10",
    "global": "4.3.2",
    "gulp": "github:gulpjs/gulp",
    "gulp-babel": "8.0.0",
    "gulp-clean-css": "3.10.0",
    "gulp-concat-util": "0.5.5",
    "gulp-copy": "4.0.1",
    "gulp-rename": "1.4.0",
    "gulp-replace": "1.0.0",
    "gulp-sass": "4.0.2",
    "gulp-sequence": "1.0.0",
    "gulp-shell": "0.6.5",
    "gulp-strip-css-comments": "2.0.0",
    "gulp-svg-sprite": "1.5.0",
    "gulp-trim": "1.0.2",
    "husky": "^1.1.3",
    "inquirer-recursive": "0.0.5",
    "jest": "23.6.0",
    "lerna": "3.4.3",
    "lint-staged": "8.0.4",
    "lit-html": "0.10.0",
    "lodash": "4.17.11",
    "lodash.clonedeep": "4.5.0",
    "node-sass": "4.10.0",
    "polymer-build": "3.1.0",
    "polymer-webpack-loader": "2.0.3",
    "prettier": "1.15.0",
    "raw-loader": "0.5.1",
    "react": "*",
    "react-dom": "*",
    "requirejs": "2.3.6",
    "rollup": "0.67.0",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-terser": "3.0.0",
    "strip-css-comments": "3.0.0",
    "trim": "0.0.1",
    "wct-browser-legacy": "1.0.2",
    "web-component-tester": "6.9.0",
    "webpack": "4.25.1",
    "whatwg-fetch": "3.0.0",
    "yo": "2.0.5"
  },
  "dependencies": {
    "@open-wc/webpack": "^0.1.4",
    "@polymer/lit-element": "^0.7.1",
    "@polymer/polymer": "3.1.0",
    "babel-loader": "^8.0.5",
    "imports-loader": "^0.8.0",
    "lit-element": "^2.0.0-rc.2",
    "slim-js": "4.0.5"
  },
  "lint-staged": {
    "linters": {
      "*.{js,json}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "elements/**/*.*.js",
      "**/analysis-error.json",
      "themes/**/*.*.js",
      "libraries/**/*.js"
    ]
  },
  "keywords": [
    "yeoman-generator",
    "custom-elements",
    "web-components",
    "webcomponents",
    "polymer",
    "lit-element",
    "skate-js",
    "slim-js",
    "twig",
    "lit-html",
    "vanillajs"
  ],
  "private": true,
  "npmClient": "yarn",
  "useWorkspaces": true,
  "workspaces": {
    "packages": [
      "elements/*",
      "themes/*"
    ],
    "nohoist": [
      "**/@lrnwebcomponents/hax-body-behaviors",
      "**/@polymer",
      "**/@polymer/**",
      "**/@rhelements/rhelement",
      "**/@skatejs",
      "**/@skatejs/**",
      "**/skatejs",
      "**/@webcomponents",
      "**/@webcomponents/**",
      "**/lit-html",
      "**/lit-html/**",
      "**/slim-js",
      "**/slim-js/**"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run test-suite-inject > /dev/null && lint-staged"
    }
  }
}
